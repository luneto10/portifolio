generator client {
  provider = "prisma-client-py"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Language {
  id        Int      @id @default(autoincrement())
  language  String
  createdAt DateTime @default(now()) @map("created_at")
}

model Project {
  id            Int      @id
  name          String
  description   String?
  html_url      String
  pushed_at     DateTime
  created_at    DateTime
  updated_at    DateTime
  languages_url String
}
